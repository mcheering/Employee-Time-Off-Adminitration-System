<h1>My Team</h1>
<table>
  <thead>
    <tr>
      <th>Name</th><th>Email</th><th>Hire Date</th>
    </tr>
  </thead>
  <tbody>
    <% @employees.each do |e| %>
      <tr>
        <td><%= e.name %></td>
        <td><%= e.email %></td>
        <td><%= e.hire_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h1> Time Off Requests </h1>
<table>
  <thead>
    <tr>
      <th>Name</th><th>Request Date</th><th>Reason</th>
    </tr>
  </thead>
  <tbody>
    <% @time_off_requests.each do |t| %>
      <tr>
        <td><%= t.name %></td>
        <td><%= t.dates %></td>
        <td><%= t.reason %></td>
        <td><%= t.reason %></td>
      </tr>
    <% end %>
  </tbody>
</table>