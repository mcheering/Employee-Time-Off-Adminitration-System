<div id="supervisor-dashboard"></div>
<%= tag.script type: "application/json", id: "supervisor-data" do %>
  <%= raw({ id: @supervisor.id, name: @supervisor.name, email: @supervisor.email }.to_json) %>
<% end %>
<%= tag.script type: "application/json", id: "fiscal-years-data" do %><%= raw @fiscal_years.to_json %><% end %>
<%= tag.script type: "application/json", id: "selected-fy" do %><%= raw @selected_fy.to_json %><% end %>
<%= tag.script type: "application/json", id: "status-options" do %><%= raw @status_options.to_json %><% end %>
<%= tag.script type: "application/json", id: "selected-status" do %><%= raw @selected_status.to_json %><% end %>
<%= tag.script type: "application/json", id: "time-off-requests" do %><%= raw @time_off_requests_payload.to_json %><% end %>
<%= tag.script type: "application/json", id: "fye-records" do %><%= raw @fye_records.to_json %><% end %>
<%= tag.script type: "application/json", id: "calendar-data" do %>
  <%= raw @calendar_data.to_json %>
<% end %>
<%= tag.script raw(@updated_requests.to_json), id: "supervisor-attention-requests", type: "application/json" %>
<%= vite_javascript_tag "entrypoints/application.jsx", type: "module" %>