<h1>Administrators</h1>
<table border="1">
  <thead>
    <tr>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>
  <tbody>
    <% @administrators.each do |admin| %>
      <tr>
        <td><%= admin.id %></td>
        <td><%= admin.first_name %></td>
        <td><%= admin.last_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'Back to Employees', employees_path %>